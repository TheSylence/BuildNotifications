                            Value="Info">
                            <Setter Property="Image.Source" Value="{StaticResource InfoDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
                            Binding="{Binding Path=Type, RelativeSource={RelativeSource AncestorType={x:Type local:IconBatch}}}"
                            Value="Lightning">
                            <Setter Property="Image.Source" Value="{StaticResource LightningDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
                            Binding="{Binding Path=Type, RelativeSource={RelativeSource AncestorType={x:Type local:IconBatch}}}"
                            Value="Checkmark">
                            <Setter Property="Image.Source" Value="{StaticResource CheckmarkDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
                            Binding="{Binding Path=Type, RelativeSource={RelativeSource AncestorType={x:Type local:IconBatch}}}"
                            Value="DownArrow">
                            <Setter Property="Image.Source" Value="{StaticResource DownArrowDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
                            Binding="{Binding Path=Type, RelativeSource={RelativeSource AncestorType={x:Type local:IconBatch}}}"
                            Value="Project">
                            <Setter Property="Image.Source" Value="{StaticResource ProjectDrawingImage}" />
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </Image.Style>
        </Image>
    </Viewbox>
</UserControl>