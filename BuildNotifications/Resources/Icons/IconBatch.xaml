                            Value="Text">
                            <Setter Property="Image.Source" Value="{StaticResource TextDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
                            Binding="{Binding Path=Type, RelativeSource={RelativeSource AncestorType={x:Type local:IconBatch}}}"
                            Value="Plus">
                            <Setter Property="Image.Source" Value="{StaticResource PlusDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
                            Binding="{Binding Path=Type, RelativeSource={RelativeSource AncestorType={x:Type local:IconBatch}}}"
                            Value="Arrow">
                            <Setter Property="Image.Source" Value="{StaticResource ArrowDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
                            Binding="{Binding Path=Type, RelativeSource={RelativeSource AncestorType={x:Type local:IconBatch}}}"
                            Value="Minus">
                            <Setter Property="Image.Source" Value="{StaticResource MinusDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
                            Binding="{Binding Path=Type, RelativeSource={RelativeSource AncestorType={x:Type local:IconBatch}}}"
                            Value="Dummy">
                            <Setter Property="Image.Source" Value="{StaticResource DummyDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
                            Binding="{Binding Path=Type, RelativeSource={RelativeSource AncestorType={x:Type local:IconBatch}}}"
                            Value="Notification">
                            <Setter Property="Image.Source" Value="{StaticResource NotificationsDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
                            Binding="{Binding Path=Type, RelativeSource={RelativeSource AncestorType={x:Type local:IconBatch}}}"
                            Value="SingleBuild">
                            <Setter Property="Image.Source" Value="{StaticResource GroupingSoloDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
                            Binding="{Binding Path=Type, RelativeSource={RelativeSource AncestorType={x:Type local:IconBatch}}}"
                            Value="Pause">
                            <Setter Property="Image.Source" Value="{StaticResource PauseDrawingImage}" />
                        </DataTrigger>

                        <DataTrigger
