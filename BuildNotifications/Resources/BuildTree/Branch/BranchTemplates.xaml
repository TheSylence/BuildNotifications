<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModelTree="clr-namespace:BuildNotifications.ViewModel.Tree">

    <DataTemplate x:Key="BranchGroupNodeTemplate" DataType="{x:Type viewModelTree:BranchGroupNodeViewModel}">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
            <TextBlock Text="{Binding BranchName}" FontSize="{DynamicResource BlockFont}" Foreground="{DynamicResource Foreground1}" UseLayoutRounding="True"/>
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>